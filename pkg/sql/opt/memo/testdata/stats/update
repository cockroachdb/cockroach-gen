 │         ├── key: (4)
 │         ├── fd: ()-->(6,7), (4)-->(5)
 │         ├── select
 │         │    ├── columns: xyz.x:4(string!null) xyz.y:5(int!null) xyz.z:6(float!null)
 │         │    ├── stats: [rows=10, distinct(4)=10, null(4)=0, distinct(6)=1, null(6)=0]
