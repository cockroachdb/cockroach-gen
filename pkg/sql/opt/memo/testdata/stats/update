 │                        │    └── key: (9)
 │                        └── filters
 │                             └── p:8 = parent.p:9 [type=bool, outer=(8,9), constraints=(/8: (/NULL - ]; /9: (/NULL - ]), fd=(8)==(9), (9)==(8)]
 └── select
      ├── columns: p:10(int!null)
      ├── stats: [rows=1000, distinct(10)=1, null(10)=0]
      ├── fd: ()-->(10)
      ├── with-scan &2 (q)
